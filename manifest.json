{
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "browser_action": {
    "default_popup": "src/html/popup.html",
    "default_icon": {
      "128": "src/icons/extension.png"
    }
  },
  "description": "Add to issue page two buttons which allow copy branch/commit message",
  "homepage_url": "https://github.com/ad/ad/blob/master/README.md",
  "icons": {
    "128": "src/icons/extension.png"
  },
  "manifest_version": 2,
  "key": "",
  "name": "Copy branch/commit message",
  "update_url": "https://raw.githubusercontent.com/ad/ad/master/updates.xml",
  "short_name": "Copy branch/commit message",
  "permissions": [
    "clipboardWrite",
    "<all_urls>"
  ],
  "version": "0.0.1"
}
