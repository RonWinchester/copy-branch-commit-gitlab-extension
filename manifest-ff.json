{
  "content_scripts": [
    {
    "matches": [
    "<all_urls>"
    ],
      "js": ["src/js/contentScript.js"]
    }
  ],
  "applications": {
    "gecko": {
      "id": "ad@ad",
      "update_url": "https://raw.githubusercontent.com/ad/ad/master/updates.json"
    }
  },
  "browser_action": {
    "theme_icons": [
      {
        "light": "src/icons/extension-dark.png",
        "dark": "src/icons/extension.png",
        "size": 128
      }
    ],
    "default_popup": "src/html/popup.html",
    "default_icon": {
      "128": "src/icons/extension.png"
    }
  },
  "description": "Add to issue page two buttons which allow copy branch/commit message",
  "homepage_url": "https://github.com/ad/ad/blob/master/README.md",
  "icons": {
    "128": "src/icon/extension.png"
  },
  "manifest_version": 2,
  "name": "Copy branch/commit message",
  "short_name": "Copy branch/commit message",
  "permissions": [
    "clipboardWrite",
    "<all_urls>"
  ],
  "version": "0.0.1"
}
